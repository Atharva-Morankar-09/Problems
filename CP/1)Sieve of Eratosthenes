
//  0 - Prime
//  1 - Not prime

#include<bits/stdc++.h>
using namespace std;

int main()
{
  int maxN = 100000001;
  vector<int> prime(maxN,0);
  vector<int> ans;
  prime[0]=prime[1]=1;
  for(int i=2;i*i<=maxN;i++)
  {
    if(prime[i]==0)
    {
       for(int j=i*i;j<=maxN;j+=i)
       {
           prime[j]=1;
       }
    }
  }

  for(int i=0;i<maxN;i++)
  {
   if(prime[i]==0)
   {
      ans.push_back(prime[i]);
   }
  }
 return 0; 
}

T.C. - O(nlog(logn))
